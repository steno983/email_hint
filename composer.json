{
  "name": "steno983/email_hint",
  "authors": [
    {
      "name": "Stefano Siciliano",
      "email": "stefano.siciliano@gmail.com",
      "role": "Developer"
    }
  ],
  "version": "0.1",
  "description": "hint email",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "illuminate/contracts": "^10.0",
    "illuminate/support": "^10.0",
    "laravel/prompts": "v0.1.13"
  },
  "require-dev": {
    "orchestra/testbench": "8.20.0|^9.0",
    "pestphp/pest": "^2.0"
  },
  "autoload": {
    "files": [
    ],
    "psr-4": {
      "Steno983\\EmailHint\\": "src",
      "Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Steno983\\EmailHint\\EmailHintServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
